{
  "name": "virgolauncher",
  "homepage": "./",
  "main": "./public/electron.js",
  "description": "l0svirgosdelautu official Minecraft client launcher",
  "author": "l0svirgosdelautu",
  "build": {
    "appId": "<com.virgolauncher>",
    "win": {
      "target": "nsis",
      "asar": false
    }
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "concurrently": "^6.0.2",
    "electron-is-dev": "^2.0.0",
    "gn": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "wait-on": "^5.3.0",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm react-build && electron-builder --publish=always",
    "build": "react-scripts build && electron-builder --win",
    "start": "concurrently \"react-scripts start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^12.0.7",
    "electron-packager": "^15.2.0",
    "electron-builder": "^22.11.1"
  }
}
